{
  "name": "bs-compile-middleware",
  "version": "0.1.1",
  "author": "katashin",
  "description": "Compilation middleware for Browsersync",
  "keywords": [
    "browsersync",
    "compile",
    "middleware"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/ktsn/bs-compile-middleware",
  "bugs": "https://github.com/ktsn/bs-compile-middleware/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/ktsn/bs-compile-middleware.git"
  },
  "scripts": {
    "prepublish": "npm run release",
    "clean": "rimraf -rf lib",
    "dev": "npm run test:unit -- -w",
    "example": "npm run build && node example/server.js",
    "build": "babel src --out-dir lib",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "flow": "flow check",
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "test": "npm run lint && npm run flow && npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test mocha --compilers js:babel-register test",
    "release": "npm run test && npm run clean && npm run build:prod"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-power-assert": "^1.0.0",
    "browser-sync": "^2.18.2",
    "buble": "^0.14.2",
    "cross-env": "^3.0.0",
    "eslint": "^3.0.0",
    "eslint-config-ktsn": "^1.0.0",
    "eslint-plugin-flowtype": "^2.4.1",
    "flow-bin": "^0.36.0",
    "mocha": "^3.2.0",
    "node-sass": "^3.13.0",
    "power-assert": "^1.3.1",
    "pug": "^2.0.0-beta6",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "mime": "^1.3.4"
  }
}
